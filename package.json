{
  "name": "arc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "env:staging": "cross-env NODE_ENV=staging",
    "lint": "eslint src src-example private webpack.config.js",
    "prod": "npm run env:prod -- webpack-dev-server",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "build:data": "fab combineDataFiles",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack",
    "build:staging": "npm run env:staging -- webpack",
    "deploy": "npm run deploy:prepare && npm run deploy:update && npm run deploy:push",
    "deploy:prepare": "npm run build:clean && npm run build",
    "deploy:prepare:staging": "npm run build:clean && npm run build:staging",
    "deploy:update": "git add dist && git commit -m \"Update dist\"",
    "deploy:push": "git subtree push --prefix dist origin gh-pages",
    "deploy:staging": "npm run deploy:prepare:staging && npm run deploy:surge",
    "deploy:surge": "surge -p dist -d cedej-atlas.surge.sh"
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
